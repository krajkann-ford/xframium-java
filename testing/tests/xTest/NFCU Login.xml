<suite xmlns="http://www.xframium.org/testRegistry" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.xframium.org/testRegistry http://www.xframium.org/xsd/pageManagement/testRegistry.xsd">
  <test name="NFCU Login" timed="false" threshold="0" active="true">
    <description>This is a check for a valid login</description>
    <step name="NAVIGATE" site="xTest" page="home" type="BROWSER" active="true" threshold="0" timed="false" inverse="false" wait="0">
      <parameter type="STATIC" name="The URL to navigate to" value="http://nfcu.com" />
    </step>
    <step name="user" site="NFCU" page="Home" type="SET" active="true" threshold="0" timed="false" inverse="false" wait="0">
      <parameter type="STATIC" name="Value to Set" value="Allen Geary" />
      <parameter type="STATIC" name="Set Method" value="DEFAULT" />
    </step>
    <step name="password" site="NFCU" page="Home" type="SET" active="true" threshold="0" timed="false" inverse="false" wait="0">
      <parameter type="STATIC" name="Value to Set" value="jhsdhsdhshsdf" />
      <parameter type="STATIC" name="Set Method" value="DEFAULT" />
    </step>
    <step name="signin" site="NFCU" page="Home" type="CLICK" active="true" threshold="0" timed="false" inverse="false" wait="0" />
    <step name="Error Header" site="NFCU" page="Failed Login" type="VISIBLE" active="true" threshold="0" timed="false" inverse="false" wait="0" failureMode="IGNORE">
      <step name="BACK" site="xTest" page="home" type="BROWSER" active="true" threshold="0" timed="false" inverse="false" wait="0" />
      <step name="END_FAILURE" site="xTest" page="home" type="FLOW" active="true" threshold="0" timed="false" inverse="false" wait="0" />
    </step>
    <step name="Undefined" site="xTest" page="home" type="REPORT" active="true" threshold="0" timed="false" inverse="false" wait="0">
      <parameter type="STATIC" name="Item One" value="Report Data" />
    </step>
    
    <step name="xxx" type="COMPARE2">
      <parameter type="CONTEXT" value="_device.model" />
      <parameter type="STATIC" value="i-Phone" />
    </step>
    
  </test>
</suite>
